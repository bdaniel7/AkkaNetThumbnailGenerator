akka 
{
	stdout-loglevel = ERROR
	loglevel = ERROR
	log-config-on-start = on   

	actor 
	{
		debug 
		{  
			  receive = on 
			  autoreceive = on
			  lifecycle = on
			  event-stream = on
			  unhandled = on
		}
		deployment
		{
            /startReadingPostsActor
            {
                router = round-robin-pool
                nr-of-instances = 20
            }
		}
	}
	remote
	{
		dot-netty.tcp 
		{
			port = 0 
			hostname = localhost
		}
	}
}
